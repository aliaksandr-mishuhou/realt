{
  "RepositoryServiceUrl": "http://localhost:8080",
  "ExecutionTime": "now",

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Http",
        "Args": {
          "requestUri": "http://localhost:5000",
        }
      },
      {
        "Name": "Console"
      }
    ]
  },

  "AllowedHosts": "*"
}
